#!/usr/bin/env bash
# A script that displays "Best School" 10 times, with "Hi" on a new line 
for the 9th iteration, using a while loop and if statement

count=1
while [ $count -le 10 ]
do
    if [ $count -eq 9 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    ((count++))
done
